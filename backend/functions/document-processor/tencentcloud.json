{
  "CNAM": {
    "actions": [],
    "environment": {
      "variables": {
        "TENCENT_COS_SECRET_ID": "",
        "TENCENT_COS_SECRET_KEY": "",
        "TENCENT_COS_BUCKET": "",
        "TENCENT_COS_REGION": "ap-guangzhou",
        "QDRANT_URL": "http://your-qdrant-server:6333",
        "ONEAPI_BASE_URL": "http://your-oneapi",
        "ONEAPI_KEY": "sk-xxx",
        "EMBEDDING_MODEL": "BAAI/bge-m3",
        "CALLBACK_BASE_URL": "https://your-domain.com"
      }
    },
    "handler": "index.handler",
    "memorySize": 2048,
    "runtime": "Nodejs20",
    "timeout": 300,
    "role": "QCS_SCFRole",
    "triggers": [
      {
        "enable": true,
        "qualifier": "$DEFAULT",
        "type": "ckafka",
        "triggerDesc": {
          "connection": {
            "ckafka_instance_id": "your-instance-id",
            "ckafka_topic": "context-doc-process",
            "offset": "latest",
            "recordSchema": "JSON"
          }
        }
      }
    ]
  }
}
