NODE_ENV=production
PORT=3000

# LiteLLM (local on server A)
LITELLM_BASE_URL=http://127.0.0.1:{{LITELLM_PORT}}
LITELLM_API_KEY=REPLACE_ME

# Qdrant (server B internal IP)
QDRANT_URL=http://{{QDRANT_INTERNAL_IP}}:6333
# QDRANT_ROOT_KEY=REPLACE_ME

# Redis / Worker
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
WORKER_CONCURRENCY=1

# Models
EMBEDDING_MODEL=qwen3-embedding-4b
RERANK_MODEL=qwen3-reranker-4b
CHAT_MODEL=qwen3-max
VISION_OCR_MODEL=qwen3-vl-flash

# OCR image pipeline
PYMUPDF_IMAGE_SCALE=0.8
PYMUPDF_IMAGE_FORMAT=jpeg
PYMUPDF_IMAGE_JPEG_QUALITY=82
PYMUPDF_IMAGE_STREAM_MODE=file

# COS storage
TENCENT_COS_SECRET_ID=REPLACE_ME
TENCENT_COS_SECRET_KEY=REPLACE_ME
TENCENT_COS_BUCKET=REPLACE_ME
TENCENT_COS_REGION=ap-guangzhou

# Security
JWT_SECRET=REPLACE_ME_LONG_RANDOM
ALLOWED_ORIGINS=https://{{DOMAIN}},https://{{WWW_DOMAIN}}
